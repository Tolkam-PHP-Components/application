{
    "name": "tolkam/application",
    "description": "PSR-15/CLI applications",
    "type": "library",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Tolkam\\Application\\": "src"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-mbstring": "*",
        "psr/http-message": "~1.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-handler": "~1.0",
        "psr/http-server-middleware": "~1.0",
        "tolkam/psr15-dispatcher": "^1.0@dev",
        "symfony/console": "^5.0",
        "symfony/process": "^5.0",
        "symfony/lock": "^5.0"
    },
    "suggest": {
        "tolkam/application-extras": "Useful middlewares, factories and more",
        "tolkam/application-boilerplate-web": "Common web application boilerplate"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
